{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Next.js",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/next",
      "runtimeArgs": ["dev"],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "restart": true,
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
